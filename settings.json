{
    "botNumber": 0,
    "masterNumber": 0,
    "botHeartbeatGroupNumber": 0,
    "botHeartbeatInterval": 300,

    "requestTimeout": 10,
    "requestUserAgent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/92.0.4515.131 Safari/537.36",
    "requestUserAgentMobile": "Mozilla/5.0 (Linux; Android 11; SM-A205U) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/92.0.4515.131 Mobile Safari/537.36",

    "thumbnailDownloadTimeout": 10,
    "thumbnailMaximumSize": {
        "width": 250,
        "height": 250,
        "fit": "inside"
    },

    "miraiRoot": "/absolute/path/to/mirai",
    "miraiHttpConfigPath": "./config/net.mamoe.mirai-api-http/setting.yml",
    "miraiHttpCachePath": "./data/net.mamoe.mirai-api-http/mirai-mybot/cache",

    "extra": {
        "twitter": {
            "bearerToken": "token",
            "video": {
                "fileSizeLimitBytes": 5242880,
                "highQualityDurationLimitMilliseconds": 8000,
                "highQuality": {
                    "frameRate": 10,
                    "width": 250,
                    "height": 250
                },
                "lowQuality": {
                    "frameRate": 8,
                    "width": 150,
                    "height": 150
                }
            }
        }
    }
